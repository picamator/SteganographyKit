{
    "name": "picamator/steganographykit",
    "description": "Implementation several steganography algorithms",
    "keywords": ["steganography", "cryptography"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergii Pryz",
            "email": "picamator@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/picamator/SteganographyKit/issues"
    },
    "autoload": {
        "psr-4": {
            "Picamator\\SteganographyKit\\": "src/"
        },
        "exclude-from-classmap": [
            "/dev/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Picamator\\SteganographyKit\\Tests\\Unit\\": "tests/unit/src",
            "Picamator\\SteganographyKit\\Tests\\Integration\\": "tests/integration/src"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~5.6|~7.0",
        "ext-zip": "*",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "phpdocumentor/phpdocumentor": "~2.0",
        "satooshi/php-coveralls": "~1.0"
    }
}
